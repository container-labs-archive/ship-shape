{
  "name": "Api-graphql",
  "description": "Api-graphql for Firebase",
  "scripts": {
    "start": "funpack -e local && node functions/index-docker.js",
    "start-alpha": "funpack -e alpha && node functions/index-docker.js",
    "start-docker": "funpack -e staging && node functions/index-docker.js",
    "start-docker-prod": "funpack -e production && node functions/index-docker.js",
    "dev": "export GCLOUD_PROJECT=libra-staging ; nodemon start --watch functionsES6",
    "dev-alpha": "nodemon --exec 'npm run start-alpha' --watch functionsES6",
    "flow": "flow",
    "lint": "eslint functionsES6/* ",
    "lint-fix": "eslint functionsES6/* --fix",
    "pretest": "funpack -e local",
    "test": "jest functions/*",
    "copy-package": "cp package.json functionsES6/package.json && cp yarn.lock functionsES6/yarn.lock",
    "package-functions-prod": "yarn run copy-package && funpack -e production",
    "package-functions-staging": "yarn run copy-package && funpack -e staging",
    "package-functions-local": "yarn run copy-package && funpack -e local",
    "build-staging": "funpack -e staging && cd functions && yarn install",
    "build-alpha": "funpack -e staging && cd functions",
    "fix": "eslint --fix functionsES6/*",
    "predeploy-staging": "funpack -e staging",
    "deploy-staging": "firebase deploy --project gpt-staging --only functions:graphql"
  },
  "node": "10",
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@containerlabs/shared-nodejs-function": "^1.17.0",
    "@google-cloud/firestore": "^4.4.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "core-js": "^3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "firebase-admin": "^9.0.0",
    "firebase-functions": "^3.11.0",
    "firebase-tools": "^8.12.0",
    "global": "^4.4.0",
    "graphql": "^15.3.0",
    "graphql-playground-middleware-express": "^1.7.21",
    "graphql-tools": "^6.2.4",
    "helmet": "^4.1.1",
    "http-errors": "1.8.0",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-proposal-export-namespace-from": "7.10.4",
    "@babel/plugin-proposal-function-sent": "7.10.4",
    "@babel/plugin-proposal-json-strings": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-destructuring": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/plugin-transform-spread": "7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@containerlabs/funpack": "^1.13.0",
    "@firebase/app-types": "0.6.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "flow-bin": "^0.135.0",
    "jest": "^26.4.2"
  },
  "sideEffects": false,
  "private": true
}
