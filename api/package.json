{
  "name": "Api-graphql",
  "description": "Api-graphql for Firebase",
  "scripts": {
    "start": "funpack -e local && node functions/index-docker.js",
    "start-alpha": "funpack -e alpha && node functions/index-docker.js",
    "start-docker": "funpack -e staging && node functions/index-docker.js",
    "start-docker-prod": "funpack -e production && node functions/index-docker.js",
    "dev": "export GCLOUD_PROJECT=libra-staging ; nodemon start --watch functionsES6",
    "dev-alpha": "nodemon --exec 'npm run start-alpha' --watch functionsES6",
    "flow": "flow",
    "pretest": "funpack -e local",
    "test": "jest functions/*",
    "package-functions-production": "funpack -e production",
    "package-functions-staging": "funpack -e staging",
    "package-functions-local": "funpack -e local",
    "package-functions-alpha": "funpack -e alpha",
    "build-staging": "funpack -e staging && cd functions && yarn install",
    "build-alpha": "funpack -e staging && cd functions",
    "fix": "eslint --fix functionsES6/*"
  },
  "node": "10",
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@babel/runtime": "7.2.0",
    "@containerlabs/react-apollo-shared": "^2.4.0",
    "@google-cloud/firestore": "^3.4.1",
    "@google-cloud/profiler": "^1.0.1",
    "@google-cloud/trace-agent": "^3.2.1",
    "@sendgrid/mail": "^6.3.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "core-js": "^3.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "firebase-admin": "^8.0.0",
    "firebase-functions": "^3.3.0",
    "firebase-tools": "^7.12.1",
    "global": "^4.4.0",
    "graphql": "^14.4.1",
    "graphql-playground-middleware-express": "^1.7.15",
    "graphql-tools": "^6.0.5",
    "helmet": "^3.22.0",
    "http-errors": "1.7.1",
    "lodash": "^4.17.11",
    "morgan": "^1.10.0",
    "raven": "^2.6.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-proposal-decorators": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-destructuring": "7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-transform-spread": "7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@containerlabs/funpack": "^1.0.0",
    "@firebase/app-types": "0.3.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "flow-bin": "^0.87.0",
    "jest": "^23.6.0"
  },
  "sideEffects": false,
  "private": true
}
